# quick virtual environment activation from current folder
alias venv="source .venv/bin/activate"
# play audio only
alias mpa="mpv --no-video"
# ugly hack
alias vim="nvim"
# can't remember "zellij"
alias zmux="zellij"
# cat images with img2sixel
alias icat="img2sixel"
# mail
alias mailsync="python3 ~/mail/syncer.py"
alias mutt="neomutt"
# matrix
alias elm="iamb"
# w3m with sixel images
alias w3im="w3m -o display_image=true -o inline_img_protocol=2"
